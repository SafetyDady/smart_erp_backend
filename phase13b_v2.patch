++from sqlalchemy.ext.declarative import declarative_base
++from sqlalchemy.orm import sessionmaker, Session, relationship
++
++# --- Configuration ---
++DATABASE_URL = os.getenv("DATABASE_URL", "sqlite:///./test.db")
++if DATABASE_URL.startswith("postgres://"):
++    DATABASE_URL = DATABASE_URL.replace("postgres://", "postgresql://", 1)
++
++ENV = os.getenv("RAILWAY_ENVIRONMENT", "development")
++LOW_STOCK_THRESHOLD = int(os.getenv("LOW_STOCK_THRESHOLD", "10"))
++
++# --- Database Setup ---
++engine = create_engine(DATABASE_URL)
++SessionLocal = sessionmaker(autocommit=False, autoflush=False, bind=engine)
++Base = declarative_base()
++
++# --- Enums ---
++class ProductType(str, Enum):
++    FINISHED = 'FINISHED'
++    MATERIAL = 'MATERIAL'
++    CONSUMABLE = 'CONSUMABLE'
++
++class MovementType(str, Enum):
++    RECEIVE = 'RECEIVE'
++    ISSUE = 'ISSUE'
++    CONSUME = 'CONSUME'
++    ADJUST = 'ADJUST'
++
++class UserRole(str, Enum):
++    OWNER = 'OWNER'
++    MANAGER = 'MANAGER'
++    STAFF = 'STAFF'
++
++# --- Mock Auth Service (Server-Side Resolution) ---
++MOCK_USER_DB = {
++    "user_owner": UserRole.OWNER,
++    "user_manager": UserRole.MANAGER,
++    "user_staff": UserRole.STAFF
++}
++
++def get_current_role(x_user_id: str = Header(..., alias="X-User-ID")) -> UserRole:
++    role = MOCK_USER_DB.get(x_user_id)
++    if not role:
++        raise HTTPException(status_code=401, detail="Invalid User ID")
++    return role
++
++# --- SQLAlchemy Models ---
++class ProductModel(Base):
++    __tablename__ = "products"
++    id = Column(String, primary_key=True, index=True, default=lambda: str(uuid.uuid4()))
++    code = Column(String, unique=True, index=True, nullable=False)
++    name = Column(String, nullable=False)
++    type = Column(SQLEnum(ProductType), nullable=False)
++    cost = Column(Float, nullable=False)
++    price = Column(Float, nullable=False)
++    unit = Column(String, nullable=False)
++    is_active = Column(Boolean, default=True)
++    
++    balance = relationship("StockBalanceModel", uselist=False, back_populates="product", cascade="all, delete-orphan")
++
++class StockBalanceModel(Base):
++    __tablename__ = "stock_balances"
++    product_id = Column(String, ForeignKey("products.id"), primary_key=True)
++    on_hand = Column(Integer, default=0, nullable=False)
++    last_updated = Column(DateTime, default=datetime.utcnow, onupdate=datetime.utcnow)
++    
++    product = relationship("ProductModel", back_populates="balance")
++
++class StockMovementModel(Base):
++    __tablename__ = "stock_movements"
++    id = Column(String, primary_key=True, index=True, default=lambda: str(uuid.uuid4()))
++    product_id = Column(String, ForeignKey("products.id"), nullable=False)
++    type = Column(SQLEnum(MovementType), nullable=False)
++    quantity = Column(Integer, nullable=False)
++    balance_after = Column(Integer, nullable=False)
++    reason = Column(String, nullable=True) # 'note'
++    created_by = Column(String, nullable=False) # 'performed_by'
++    created_at = Column(DateTime, default=datetime.utcnow) # 'performed_at'
++
++# --- Pydantic Schemas ---
++class ProductCreate(BaseModel):
++    code: str
++    name: str
++    type: ProductType
++    cost: float
++    price: float
++    unit: str
++
++class ProductResponse(ProductCreate):
