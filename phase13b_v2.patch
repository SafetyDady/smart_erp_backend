++  baseURL: API_URL,
++  headers: {
++    'Content-Type': 'application/json',
++  },
++});
++
++// Mock User ID Injection (In real app, this comes from Auth Context)
++// Change this to 'user_manager' or 'user_staff' to test other roles
++const CURRENT_USER_ID = 'user_owner'; 
++
++export const InventoryService = {
++  async getProducts(): Promise<Product[]> {
++    const response = await api.get<Product[]>('/products');
++    return response.data;
++  },
++
++  async getStats(): Promise<InventoryStats> {
++    const response = await api.get<InventoryStats>('/dashboard/stats', {
++      headers: { 'X-User-ID': CURRENT_USER_ID } // Send User ID, NOT Role
++    });
++    return response.data;
++  },
++
++  async createProduct(product: any): Promise<Product> {
++    const response = await api.post<Product>('/products', product, {
++      headers: { 'X-User-ID': CURRENT_USER_ID }
++    });
++    return response.data;
++  },
++
++  async createMovement(
++    movement: {
++      product_id: string;
++      type: MovementType;
++      quantity: number;
++      reason?: string;
++    }
++  ): Promise<StockMovement> {
++    const response = await api.post<StockMovement>('/movements', movement, {
++      headers: { 'X-User-ID': CURRENT_USER_ID }
++    });
++    return response.data;
++  }
++};
