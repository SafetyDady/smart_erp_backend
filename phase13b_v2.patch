+import React, { useEffect, useState } from 'react';
+import { InventoryService } from '../../services/inventoryService';
+import { InventoryStats, UserRole } from '../../types/inventory';
+import { Card, CardContent, CardHeader, CardTitle } from '../ui/card';
+import { Package, AlertTriangle, DollarSign } from 'lucide-react';
+
+interface InventoryWidgetsProps {
+  role: UserRole;
+}
+
+export function InventoryWidgets({ role }: InventoryWidgetsProps) {
+  const [stats, setStats] = useState<InventoryStats | null>(null);
+  const [isLoading, setIsLoading] = useState(true);
+
+  useEffect(() => {
+    const fetchStats = async () => {
+      try {
+        const data = await InventoryService.getStats(role);
+        setStats(data);
+      } catch (error) {
+        console.error('Failed to fetch inventory stats', error);
+      } finally {
+        setIsLoading(false);
+      }
+    };
+
+    fetchStats();
+  }, [role]);
+
+  if (isLoading) {
+    return <div className="grid grid-cols-1 md:grid-cols-3 gap-4 animate-pulse">
+      {[1, 2, 3].map(i => <div key={i} className="h-32 bg-slate-100 rounded-lg"></div>)}
+    </div>;
+  }
+
+  if (!stats) return null;
+
+  return (
+    <div className="grid grid-cols-1 md:grid-cols-3 gap-4 mb-8">
+      {/* Total Products Widget */}
+      <Card>
+        <CardHeader className="flex flex-row items-center justify-between space-y-0 pb-2">
+          <CardTitle className="text-sm font-medium">Total Products</CardTitle>
