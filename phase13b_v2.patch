++        orm_mode = True
++
++class MovementCreate(BaseModel):
++    product_id: str
++    type: MovementType
++    quantity: int
++    reason: Optional[str] = None
++
++class DashboardStats(BaseModel):
++    total_products: int
++    low_stock_count: int
