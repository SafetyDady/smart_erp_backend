+import os
+import uuid
+from datetime import datetime
+from typing import List, Optional
+from enum import Enum
+
+from fastapi import FastAPI, HTTPException, Depends, Header
+from fastapi.middleware.cors import CORSMiddleware
+from pydantic import BaseModel
+from sqlalchemy import create_engine, Column, String, Integer, Float, Boolean, DateTime, Enum as SQLEnum, ForeignKey
+from sqlalchemy.ext.declarative import declarative_base
+from sqlalchemy.orm import sessionmaker, Session, relationship
+
+# --- Configuration ---
+DATABASE_URL = os.getenv("DATABASE_URL", "sqlite:///./test.db")
+if DATABASE_URL.startswith("postgres://"):
+    DATABASE_URL = DATABASE_URL.replace("postgres://", "postgresql://", 1)
+
+ENV = os.getenv("RAILWAY_ENVIRONMENT", "development")
+LOW_STOCK_THRESHOLD = int(os.getenv("LOW_STOCK_THRESHOLD", "10"))
+
+# --- Database Setup ---
+engine = create_engine(DATABASE_URL)
+SessionLocal = sessionmaker(autocommit=False, autoflush=False, bind=engine)
+Base = declarative_base()
+
+# --- Enums ---
+class ProductType(str, Enum):
+    FINISHED = 'FINISHED'
+    MATERIAL = 'MATERIAL'
+    CONSUMABLE = 'CONSUMABLE'
+
+class MovementType(str, Enum):
+    RECEIVE = 'RECEIVE'
+    ISSUE = 'ISSUE'
+    CONSUME = 'CONSUME'
+    ADJUST = 'ADJUST'
+
+class UserRole(str, Enum):
+    OWNER = 'OWNER'
+    MANAGER = 'MANAGER'
+    STAFF = 'STAFF'
+
+# --- Mock Auth Service (Server-Side Resolution) ---
+MOCK_USER_DB = {
+    "user_owner": UserRole.OWNER,
+    "user_manager": UserRole.MANAGER,
+    "user_staff": UserRole.STAFF
+}
+
+def get_current_role(x_user_id: str = Header(..., alias="X-User-ID")) -> UserRole:
+    role = MOCK_USER_DB.get(x_user_id)
+    if not role:
+        raise HTTPException(status_code=401, detail="Invalid User ID")
+    return role
+
+# --- SQLAlchemy Models ---
+class ProductModel(Base):
+    __tablename__ = "products"
+    id = Column(String, primary_key=True, index=True, default=lambda: str(uuid.uuid4()))
+    code = Column(String, unique=True, index=True, nullable=False)
+    name = Column(String, nullable=False)
+    type = Column(SQLEnum(ProductType), nullable=False)
+    cost = Column(Float, nullable=False)
+    price = Column(Float, nullable=False)
+    unit = Column(String, nullable=False)
+    is_active = Column(Boolean, default=True)
+    
+    balance = relationship("StockBalanceModel", uselist=False, back_populates="product", cascade="all, delete-orphan")
+
+class StockBalanceModel(Base):
+    __tablename__ = "stock_balances"
+    product_id = Column(String, ForeignKey("products.id"), primary_key=True)
+    on_hand = Column(Integer, default=0, nullable=False)
+    last_updated = Column(DateTime, default=datetime.utcnow, onupdate=datetime.utcnow)
+    
+    product = relationship("ProductModel", back_populates="balance")
+
+class StockMovementModel(Base):
+    __tablename__ = "stock_movements"
+    id = Column(String, primary_key=True, index=True, default=lambda: str(uuid.uuid4()))
+    product_id = Column(String, ForeignKey("products.id"), nullable=False)
+    type = Column(SQLEnum(MovementType), nullable=False)
+    quantity = Column(Integer, nullable=False)
+    balance_after = Column(Integer, nullable=False)
+    reason = Column(String, nullable=True) # 'note'
+    created_by = Column(String, nullable=False) # 'performed_by'
+    created_at = Column(DateTime, default=datetime.utcnow) # 'performed_at'
+
+# --- Pydantic Schemas ---
+class ProductCreate(BaseModel):
+    code: str
+    name: str
+    type: ProductType
+    cost: float
+    price: float
+    unit: str
+
+class ProductResponse(ProductCreate):
+    id: str
+    on_hand: int
+    is_active: bool
+    
+    class Config:
+        orm_mode = True
+
+class MovementCreate(BaseModel):
+    product_id: str
+    type: MovementType
+    quantity: int
+    reason: Optional[str] = None
+
+class DashboardStats(BaseModel):
+    total_products: int
+    low_stock_count: int
+    total_stock_value: Optional[float] = None
+
+# --- App Setup ---
+app = FastAPI(title="Smart ERP Inventory Engine", version="2.2.0")
+
+# Guard: Only auto-create tables in development
+if ENV != "production":
+    Base.metadata.create_all(bind=engine)
+
+app.add_middleware(
+    CORSMiddleware,
+    allow_origins=["*"],
+    allow_credentials=True,
+    allow_methods=["*"],
+    allow_headers=["*"],
+)
+
+def get_db():
+    db = SessionLocal()
+    try:
+        yield db
+    finally:
+        db.close()
+
+# --- API Endpoints ---
+
+@app.post("/products", response_model=ProductResponse)
+def create_product(
+    product: ProductCreate, 
+    role: UserRole = Depends(get_current_role),
+    db: Session = Depends(get_db)
+):
+    if role == UserRole.STAFF:
+        raise HTTPException(status_code=403, detail="Access Denied: Staff cannot create products")
+
+    if product.type == ProductType.MATERIAL and product.cost < 1:
+        raise HTTPException(status_code=400, detail="Validation Error: Material cost must be >= 1 THB")
+
+    try:
+        db_product = ProductModel(**product.dict())
+        db.add(db_product)
+        db.flush()
+
+        db_balance = StockBalanceModel(product_id=db_product.id, on_hand=0)
+        db.add(db_balance)
+        
+        db.commit()
+        db.refresh(db_product)
+        
+        return ProductResponse(
+            **product.dict(),
+            id=db_product.id,
+            on_hand=0,
+            is_active=True
+        )
+    except Exception as e:
+        db.rollback()
+        raise e
+
+@app.get("/products", response_model=List[ProductResponse])
+def get_products(db: Session = Depends(get_db)):
+    products = db.query(ProductModel).join(StockBalanceModel).filter(ProductModel.is_active == True).all()
+    return [
+        ProductResponse(
+            id=p.id,
+            code=p.code,
+            name=p.name,
+            type=p.type,
+            cost=p.cost,
+            price=p.price,
+            unit=p.unit,
+            is_active=p.is_active,
+            on_hand=p.balance.on_hand if p.balance else 0
+        ) for p in products
+    ]
+
+@app.post("/movements")
+def create_movement(
+    movement: MovementCreate,
+    x_user_id: str = Header(..., alias="X-User-ID"),
+    role: UserRole = Depends(get_current_role),
+    db: Session = Depends(get_db)
+):
+    try:
+        balance = db.query(StockBalanceModel).filter(StockBalanceModel.product_id == movement.product_id).with_for_update().first()
+        if not balance:
+            raise HTTPException(status_code=404, detail="Product Balance not found")
+        
+        product = db.query(ProductModel).filter(ProductModel.id == movement.product_id).first()
+
+        if movement.type == MovementType.ADJUST and role != UserRole.OWNER:
+            raise HTTPException(status_code=403, detail="Access Denied: Only OWNER can ADJUST")
+        if movement.type == MovementType.RECEIVE and role == UserRole.STAFF:
+            raise HTTPException(status_code=403, detail="Access Denied: Staff cannot RECEIVE")
+
+        if movement.type == MovementType.CONSUME and product.type != ProductType.CONSUMABLE:
+            raise HTTPException(status_code=400, detail="Validation Error: CONSUME only for Consumables")
+
+        qty_change = movement.quantity
+        if movement.type in [MovementType.ISSUE, MovementType.CONSUME]:
+            qty_change = -abs(movement.quantity)
+        
+        new_balance = balance.on_hand + qty_change
+        balance.on_hand = new_balance
+
+        db_movement = StockMovementModel(
+            product_id=movement.product_id,
+            type=movement.type,
+            quantity=qty_change,
+            balance_after=new_balance,
+            reason=movement.reason,
+            created_by=x_user_id, # performed_by
+            created_at=datetime.utcnow() # performed_at
+        )
+        db.add(db_movement)
+        db.commit()
+        db.refresh(db_movement)
+        return db_movement
+
+    except Exception as e:
+        db.rollback()
+        raise e
+
+@app.get("/dashboard/stats", response_model=DashboardStats)
+def get_stats(
+    role: UserRole = Depends(get_current_role),
+    db: Session = Depends(get_db)
+):
+    results = db.query(ProductModel, StockBalanceModel).join(StockBalanceModel).filter(ProductModel.is_active == True).all()
+    
+    total_products = len(results)
+    # Configurable Threshold
+    low_stock_count = sum(1 for p, b in results if b.on_hand <= LOW_STOCK_THRESHOLD)
+    
+    total_stock_value = None
+    if role in [UserRole.OWNER, UserRole.MANAGER]:
+        total_stock_value = sum(b.on_hand * p.cost for p, b in results)
+
+    return {
+        "total_products": total_products,
+        "low_stock_count": low_stock_count,
+        "total_stock_value": total_stock_value
+    }
